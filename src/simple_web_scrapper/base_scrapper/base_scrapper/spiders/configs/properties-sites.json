  "morgansrealty": {
    "allowed_domains": ["morgansrealty.com"],
    "start_url": "https://www.morgansrealty.com/properties-for-sales",

    "listing": {
      "wait_css": "section[class='space position-relative '] div[class='container']> div.cards-main > div > div",

      "cards":       { "css": "section[class='space position-relative '] div[class='container']> div.cards-main > div > div" },
      "title":       { "css": "div > div > figcaption > a > h3::text" },
      "detail_link": { "css": "div > div > figcaption > a::attr(href)" },

      "next_button": { "css": "" },
      "next_anchor": { "css": "li[class='next'] a::attr(href)" },

      "first_card_link_css": "section[class='space position-relative '] div[class='container']> div.cards-main > div > div:nth-of-type(1)"
    },

    "detail": {
      "wait_css": "body > div:nth-child(11) > section:nth-child(2) > div:nth-child(1) > div:nth-child(1) > div:nth-child(2) > div:nth-child(1) > a",

      "fields": {
        "price":        { "css": "div[class='price-amenitity'] h3::text" },
        "currency":     { "css": "div[class='price-amenitity'] h3::text" },
        "description":  { "css": ".show_more_content::text" },
        "rent_or_buy":  { "css": "a[href='https://www.morgansrealty.com/search?prop_for=sales']::text" },
        "property_type":{ "css": ".category-label::text" },
        "coordinates":  { "css": "div.property-location iframe::attr(src)" },
        "images":       { "css": "body > div:nth-child(11) > section:nth-child(2) > div:nth-child(1) > div:nth-child(1) > div:nth-child(2) > div > a::attr(href)" },
        "size":         { "css": "body > div:nth-child(11) > section:nth-child(3) > div:nth-child(1) > div:nth-child(1) > div:nth-child(1) > div:nth-child(1) > div:nth-child(3) > div:nth-child(2) > div:nth-child(1) > p:nth-child(2)::text" },

        "sub_location": { "xpath": "normalize-space(//p[img]/text())" },
        "amenities":    { "xpath": "//div[@class='ameneties-items']/div/p/text()",
                          "get_all" : true
        },
        "bathrooms":    { "xpath": "normalize-space(//div[contains(@class,'amenity-box')][.//img[contains(@src,'/bathtub.svg')]]/p)"},
        "bedrooms":     { "xpath": "normalize-space(//div[contains(@class,'amenity-box')][.//img[contains(@src,'/bed.svg')]]/p)" },

        "location":     { "xpath": "" },
        "city":         { "xpath": "" },
        "slug":         { "xpath": "" },
        "is_poa":       { "xpath": "" },
        "year":         { "xpath": "" }
      }
    }
  },
