{
  "sanamcars": {
    "allowed_domains": ["sanamcars.com"],
    "start_url": "https://sanamcars.com/inventory/",

    "listing": {
      "wait_css": ".stm-isotope-sorting.stm-isotope-sorting-main.stm-isotope-sorting-list > div",

      "cards":       { "css": ".stm-isotope-sorting.stm-isotope-sorting-main.stm-isotope-sorting-list > div" },
      "title":       { "css": "div > div > div.meta-top >  div.title > a::text" },
      "detail_link": { "css": "div.content > div.meta-top > div.title > a::attr(href)" },

      "next_anchor": { "xpath": "//a[@class='next page-numbers']/@href" }
    },

    "detail": {
      "wait_css": "body",

      "fields": {
        "year":         { "xpath": "//td[normalize-space(text())='Year']/following-sibling::td/text()" },
        "mileage":      { "xpath": "//td[normalize-space(text())='Mileage']/following-sibling::td/text()" },
        "brand":        { "xpath": "//td[normalize-space(text())='Make']/following-sibling::td/text()" },
        "model":        { "xpath": "//td[normalize-space(text())='Model']/following-sibling::td/text()" },
        "color":        { "xpath": "//td[normalize-space(text())='Exterior']/following-sibling::td/text()" },
        "warranty":     { "xpath": "//td[normalize-space(text())='Warranty']/following-sibling::td/text()" },
        "description":  {
                          "css": "body > div:nth-child(1) > div:nth-child(4) > div:nth-child(3) > div:nth-child(1) > div:nth-child(1) > div:nth-child(1) > div:nth-child(1)  > p::text",
                          "get_all" : true
        },
        "price":        { "xpath": "//span[@class='h3']/text()" },
        "currency":     { "xpath": "//span[@class='h3']/text()" },
        "images":       { "xpath": "//a[@class='stm_fancybox']//img/@src" },

        "transmission": { "xpath": "" },
        "wheel_drive":  { "xpath": "" },
        "condition":    { "xpath": "" },
        "regional_specs": { "xpath": "" },
        "body_type":      { "xpath": "" },
        "location":       { "xpath": "" },
        "seats":          { "xpath": "" },
        "accidents":      { "xpath": "" },
        "vin":            { "xpath": "" }
      },
      "doors":       { "xpath": "" }
    }
  },
