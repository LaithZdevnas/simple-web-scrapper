      "title":       { "css": "h5.product-title a::text" },
      "detail_link": { "css": "h5.product-title a::attr(href)" },

      "fields" : {
                       "year" :           { "xpath" : "//li[contains(@class, 'icon-start')][i[contains(@class, 'fa-calendar-alt')]]/span/text()" },
